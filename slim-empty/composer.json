    "require": {
        "php": ">=5.4.0",
        "psr/log": "~1.0",
        "slim/slim": "^3.8",
        "firebase/php-jwt": "^4.0",
        "illuminate/database": "*",
        "facebook/graph-sdk": "^5.7",
        "aws/aws-sdk-php": "^3.158"
    },

    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },

    "autoload": {
        "psr-4": {
            "ElephantIO\\": ["src/", "test/"]
        },
        "classmap": [
            "controllers",
            "lib"
        ]
    },

    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}